{
  "name": "moonpocket-site",
  "version": "1.1.202510112054",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "gen:favicon": "node scripts/generate-favicon.mjs",
    "css:rtl": "node tools/build-rtl-css.mjs",
    "build": "astro build",
    "build:local": "SITE= astro build",
    "build:public": "PUBLIC_ONLY=1 astro build",
    "prebuild": "pnpm i18n:audit && pnpm i18n:check && pnpm i18n:normalize",
    "preview": "astro preview --host",
    "preview:local": "SITE= astro build && astro preview --host --port 4321",
    "check": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "vitest",
    "test:unit": "vitest run tests/unit tests/integration",
    "test:e2e": "playwright test",
    "test:all": "pnpm check && pnpm lint && pnpm test:unit && pnpm build && pnpm preview & wait-on http://localhost:4321 && pnpm test:e2e",
    "scan:i18n": "node scripts/i18n-scan.mjs",
    "backup:safe": "node scripts/backup-safe.mjs",
    "backup:moonpocket": "node scripts/backup-moonpocket-only.mjs",
    "backup:simple": "node scripts/backup-simple.mjs",
    "backup:private": "node scripts/backup-private.mjs",
    "backup:private-simple": "node scripts/backup-private-simple.mjs",
    "backup": "node scripts/backup-smart.mjs",
    "backup:push": "node scripts/backup-smart.mjs --push",
    "i18n:check": "node scripts/i18n-check.mjs",
    "i18n:dry": "node tools/i18n-pipeline.mjs",
    "i18n:apply": "node tools/i18n-apply-from-csv.mjs --locale=zh-TW",
    "i18n:apply:locale": "node tools/i18n-apply-from-csv.mjs",
    "i18n:dedupe:pages": "node tools/i18n-dedupe-pages.mjs",
    "i18n:apply+fix": "npm run i18n:apply && npm run i18n:dedupe:pages -- --locale=zh-TW",
    "i18n:check:legacy": "node scripts/check-i18n-keys.js",
    "i18n:audit": "node scripts/i18n-audit-keys.mjs",
    "i18n:audit:enhanced": "node scripts/i18n-audit-enhanced.mjs",
    "i18n:verify:zhTW": "node scripts/i18n-audit-enhanced.mjs",
    "i18n:verify": "node tools/i18n-verify.mjs",
    "i18n:verify:ar": "node tools/i18n-verify.mjs --locale=ar-SA",
    "i18n:apply:fill": "node tools/i18n-apply-from-csv.mjs --fill-missing=en-US",
    "i18n:restore": "node scripts/i18n-restore-from-csv.mjs",
    "i18n:normalize": "node scripts/i18n-normalize.mjs",
    "i18n:heal": "node scripts/i18n-heal.mjs",
    "i18n:heal:all": "node scripts/i18n-heal.mjs --lang=zh-TW && node scripts/i18n-heal.mjs --lang=zh-CN && node scripts/i18n-heal.mjs --lang=en-US",
    "i18n:report": "node scripts/i18n-report.mjs",
    "i18n:postprocess-faq": "node scripts/i18n-postprocess-faq.mjs",
    "fix:build": "pnpm i18n:heal:all && pnpm i18n:postprocess-faq && pnpm i18n:report && pnpm build:public",
    "i18n:diff": "node scripts/i18n-diff.mjs",
    "i18n:sync": "pnpm i18n:audit && pnpm i18n:restore && pnpm i18n:normalize && pnpm i18n:check",
    "i18n:sync:legacy": "node scripts/i18n-sync.mjs",
    "i18n:sync-zhCN": "node scripts/i18n-sync-zhCN.mjs",
    "i18n:sync-zhCN-from-zhTW": "node scripts/i18n-sync-zhCN-from-zhTW.mjs",
    "i18n:fix-enUS-titles": "node scripts/i18n-fix-enUS-titles.mjs",
    "i18n:extract-enUS-from-dist": "node scripts/i18n-extract-enUS-from-dist.mjs",
    "publish:public": "node scripts/publish-public.mjs",
    "publish:simple": "node scripts/publish-simple.mjs",
    "images:optimize": "node scripts/images-optimize.mjs",
    "clean:repo": "bash scripts/repo-clean.sh",
    "i18n:export": "node scripts/i18n-export.v3.mjs > i18n-export.csv",
    "i18n:import": "node scripts/i18n-import.v4.mjs ./i18n-export.csv",
    "i18n:export-3lang": "node scripts/i18n-export-3lang.mjs",
    "i18n:import-3lang": "node scripts/i18n-import-3lang.mjs",
    "dev:check": "node scripts/check-dev-ready.mjs",
    "version:bump": "node scripts/version-bump.mjs"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/react": "^3.6.2",
    "astro": "^4.15.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/unist": "^3.0.3",
    "autoprefixer": "^10.4.20",
    "axe-core": "^4.10.0",
    "cheerio": "^1.1.2",
    "csv-parse": "^6.1.0",
    "eslint": "^9.11.1",
    "eslint-plugin-astro": "^1.2.3",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "fast-csv": "^5.0.5",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.2",
    "glob": "^11.0.3",
    "husky": "^9.1.6",
    "json": "^11.0.0",
    "lint-staged": "^15.2.10",
    "micromatch": "^4.0.8",
    "minimist": "^1.2.8",
    "papaparse": "^5.5.3",
    "picocolors": "^1.1.1",
    "playwright": "^1.48.2",
    "png-to-ico": "^3.0.1",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "rtlcss": "^4.3.0",
    "sharp": "^0.34.4",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.8.1",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vitest": "^2.1.1",
    "wait-on": "^7.2.0"
  }
}
